#: guess_number.c:19
#, c-format
msgid "Choose a number from 1 to 100\n"
msgstr "Выберите число от 1 до 100\n"

#: guess_number.c:22
#, c-format
msgid "Is your number greater than %d?\n"
msgstr "Ваше число больше, чем %d?\n"

#: guess_number.c:23
#, c-format
msgid "Answer: (y/n)\n"
msgstr "Ответьте: (д/н)\n"

#: guess_number.c:33
#, c-format
msgid "Wrong input. Please, try again\n"
msgstr "Некорректный ввод. Пожалуйста, повторите\n"

#: guess_number.c:38
#, c-format
msgid "The number you chose is %d\n"
msgstr "Вы загадали число %d\n"

